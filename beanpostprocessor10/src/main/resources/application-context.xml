<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="PostProcessorXML" class="com.example.beanpostprocessor10.model.CustomBeanPostProcessor"/>

    <bean id="member1" class="com.example.beanpostprocessor10.model.Member">
        <property name="name" value="name1"/>
        <property name="surname" value="surname1"/>
        <property name="age" value="20"/>
    </bean>

    <bean id="member2" class="com.example.beanpostprocessor10.model.Member">
        <property name="name" value="name2"/>
        <property name="surname" value="surname2"/>
        <property name="age" value="22"/>
    </bean>

</beans>