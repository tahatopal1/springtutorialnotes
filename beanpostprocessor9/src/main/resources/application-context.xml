<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="member1" class="com.example.beanpostprocessor9.model.Member">
        <property name="name" value="Name1"/>
        <property name="surname" value="Surname1"/>
        <property name="age" value="30"/>
    </bean>

    <bean id="member2" class="com.example.beanpostprocessor9.model.Member">
        <property name="name" value="Name2"/>
        <property name="surname" value="Surname2"/>
        <property name="age" value="35"/>
    </bean>

    <bean id="beanFactoryProcessor" class="com.example.beanpostprocessor9.model.CustomBeanFactoryProcessor"/>

</beans>